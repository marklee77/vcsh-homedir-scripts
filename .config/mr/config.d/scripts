[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/scripts.git]
checkout = vcsh clone https://gitlab.stillwell.me/mark/vcsh-homedir-scripts.git scripts
           vcsh scripts remote set-url --push origin git@stillwell.me:mark/vcsh-homedir-scripts.git
           vcsh scripts remote set-url --add --push origin git@github.com:marklee77/vcsh-homedir-scripts.git
           vcsh scripts remote set-url --add --push origin git@bitbucket.org:marklee77/vcsh-homedir-scripts.git
